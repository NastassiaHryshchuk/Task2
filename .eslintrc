{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
      ],
    "rules": {
        "react/prop-types": 1,
        "no-unused-vars": 1,
        "arrow-parens": 0,
        "linebreak-style": 0,
        "no-path-concat": 0,
        "prefer-template": 0,
        "import/no-extraneous-dependencies": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["airbnb"]
}